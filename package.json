{
  "name": "request-logger",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "clean": "rimraf dist",
    "start": "cross-env NODE_ENV=production node ./dist/server.js",
    "serve": "cross-env NODE_ENV=development npx http-server dist -p 5001",
    "build-server": "webpack --config ./webpack.config.server.js --mode=production",
    "build-client": "webpack --config ./webpack.config.client.js --mode=production",
    "build": "npm run clean && concurrently 'npm:build-*'",
    "dev-server": "webpack --config ./webpack.config.server.js --watch --mode=development",
    "dev-client": "webpack --config ./webpack.config.client.js --watch --mode=development",
    "dev-serve": "nodemon",
    "dev": "cross-env NODE_ENV=development concurrently 'npm:dev-*'"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.4",
    "@babel/preset-react": "^7.24.7",
    "@babel/preset-typescript": "^7.24.7",
    "@types/express": "^4.17.21",
    "@types/js-cookie": "^3.0.6",
    "@types/node": "^22.6.1",
    "@types/react": "^18.3.8",
    "@types/react-dom": "^18.3.0",
    "babel-loader": "^9.2.1",
    "concurrently": "^9.0.1",
    "css-loader": "^7.1.2",
    "css-minimizer-webpack-plugin": "^7.0.0",
    "html-webpack-plugin": "^5.6.0",
    "mini-css-extract-plugin": "^2.9.1",
    "nodemon": "^3.1.7",
    "postcss-import": "^16.1.0",
    "postcss-loader": "^8.1.1",
    "postcss-preset-env": "^10.0.5",
    "rimraf": "^6.0.1",
    "ts-loader": "^9.5.1",
    "typescript": "^5.6.2",
    "webpack": "^5.94.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "axios": "^1.7.7",
    "clsx": "^2.1.1",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "express": "^4.21.0",
    "helmet": "^7.1.0",
    "js-cookie": "^3.0.5",
    "mongodb": "^5.9.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-intl": "^6.7.0",
    "react-router-dom": "^6.26.2",
    "winston": "^3.14.2",
    "zustand": "^5.0.0-rc.2"
  }
}
